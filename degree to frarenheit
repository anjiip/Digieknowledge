// Create a Method That Converts Celsius to Fahrenheit

public class temperature {
    static double t1 = 30;

    public static void tempconversion()
    {
        double newt1 = (t1 - 32) * 5 / 9;  ;
        System.out.println("temperature is: " +newt1);
    }

    public static void main(String[] args)
    {
        tempconversion();
    }

}
